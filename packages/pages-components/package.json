{
  "name": "@yext/pages-components",
  "version": "1.0.0-rc.7",
  "description": "A React component library that supports Yext Pages development",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "THIRD-PARTY-NOTICES"
  ],
  "main": "./dist/pages-components.cjs",
  "module": "./dist/pages-components.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/pages-components.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/pages-components.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^17 || ^18 || ^20"
  },
  "scripts": {
    "dev": "pnpm run storybook",
    "build": "tsc && vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "wcag": "test-storybook -c .storybook/wcag",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build",
    "update-snapshots": "test-storybook -c .storybook/snapshots -u"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/mdast": "3.0.10",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "axe-core": "^4.8.2",
    "axe-playwright": "^1.2.3",
    "generate-license-file": "^3.0.1",
    "jest-image-snapshot": "^6.4.0",
    "jsdom": "23.0.1",
    "@playwright/test": "^1.41.0",
    "playwright-core": "^1.40.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "1.0.4"
  },
  "dependencies": {
    "@lexical/code": "^0.12.5",
    "@lexical/hashtag": "^0.12.5",
    "@lexical/link": "^0.12.5",
    "@lexical/list": "^0.12.5",
    "@lexical/react": "^0.12.5",
    "@lexical/rich-text": "^0.12.5",
    "@lexical/table": "^0.12.5",
    "@lexical/utils": "^0.12.5",
    "@yext/analytics": "1.0.0-beta.5",
    "browser-or-node": "^2.1.1",
    "classnames": "^2.3.2",
    "cross-fetch": "^4.0.0",
    "lexical": "^0.12.5",
    "mdast-util-from-markdown": "1.2.0",
    "mdast-util-to-hast": "12.3.0",
    "micromark-util-chunked": "1.0.0",
    "micromark-util-classify-character": "1.0.0",
    "micromark-util-combine-extensions": "1.0.0",
    "micromark-util-resolve-all": "1.0.0",
    "micromark-util-symbol": "1.0.1",
    "micromark-util-types": "1.0.2",
    "picocolors": "^1.0.0",
    "react-markdown": "^8.0.0",
    "remark-rehype": "10.1.0",
    "unified": "11.0.4",
    "uvu": "0.5.6"
  },
  "overrides": {
    "@storybook/react-vite": {
      "vite": "^5.0.5"
    }
  }
}
