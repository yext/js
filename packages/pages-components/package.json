{
  "name": "@yext/pages-components",
  "version": "1.2.3",
  "description": "A React component library that supports Yext Pages development",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yext/js.git"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "files": [
    "dist",
    "THIRD-PARTY-NOTICES"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./style.css": "./dist/index.css"
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^18 || ^20 || ^22 || ^24"
  },
  "scripts": {
    "dev": "pnpm run storybook",
    "build": "tsc && vite build",
    "build:tsup": "tsup && pnpm build:css",
    "build:css": "lightningcss --minify --bundle --targets \">= 0.25%\" src/index.css -o dist/index.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "wcag": "test-storybook -c .storybook/wcag",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build",
    "update-snapshots": "test-storybook -c .storybook/snapshots -u"
  },
  "peerDependencies": {
    "lexical": "~0.38.2",
    "mapbox-gl": "^2.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bingmaps": "^0.0.1",
    "@types/google.maps": "^3.58.1",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/leaflet": "^1.9.12",
    "@types/luxon": "^3.7.1",
    "@types/mapbox-gl": "^3.4.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.22",
    "axe-core": "^4.8.2",
    "axe-playwright": "^1.2.3",
    "generate-license-file": "^3.0.1",
    "jest-image-snapshot": "^6.4.0",
    "jsdom": "^24.1.0",
    "lightningcss-cli": "^1.30.2",
    "picocolors": "^1.1.1",
    "playwright-core": "^1.55.1",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.6.14",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@yext/analytics": "^1.0.0",
    "browser-or-node": "^3.0.0",
    "classnames": "^2.5.1",
    "luxon": "^3.7.2"
  }
}