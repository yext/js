{
  "name": "@yext/pages-components",
  "private": true,
  "version": "0.0.0",
  "description": "A components library for React components to use in Yext Pages development",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "type": "module",
  "files": [
    "dist",
    "THIRD-PARTY-NOTICES"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pages-components.js",
      "require": "./dist/pages-components.umd.js"
    }
  },
  "engines": {
    "node": "^17 || ^18 || ^19"
  },
  "scripts": {
    "dev": "npm run storybook",
    "build": "tsc && vite build",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "eslint --cache --fix src/**",
    "storybook": "storybook dev -p 6006 -h localhost",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/blocks": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.3",
    "@types/react": "^17.0.49",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.26",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.1.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@yext/analytics": "^0.6.1",
    "@yext/components-tsx-geo": "^1.0.3",
    "@yext/components-tsx-maps": "^1.0.11",
    "browser-or-node": "^2.1.1",
    "classnames": "^2.3.2",
    "picocolors": "^1.0.0",
    "cross-fetch": "^4.0.0"
  }
}
