{
  "name": "@yext/sites-components",
  "version": "1.0.0-rc.5",
  "description": "A React component library that supports Yext Pages development",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "keywords": [
    "Yext",
    "React"
  ],
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "THIRD-PARTY-NOTICES"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sites-components.js",
      "require": "./dist/sites-components.umd.js"
    }
  },
  "engines": {
    "node": "^17 || ^18 || ^19"
  },
  "scripts": {
    "dev": "pnpm run storybook",
    "build": "tsc && vite build",
    "fmt": "prettier --write --cache \"src/**/**.{ts,tsx,js,jsx}\"",
    "lint": "eslint --cache --fix src/**",
    "storybook": "storybook dev -p 6006 -h localhost",
    "build-storybook": "storybook build",
    "test": "jest",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "prepare": "pnpm build"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/blocks": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/react-vite": "^7.0.26",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.26",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.1.0",
    "vite-tsconfig-paths": "^4.2.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "ts-jest": "^29.1.1",
    "ts-jest-mock-import-meta": "^1.0.0",
    "identity-obj-proxy": "^3.0.0"
  },
  "dependencies": {
    "@yext/analytics": "^0.3.0",
    "browser-or-node": "^2.1.1",
    "classnames": "^2.3.2",
    "picocolors": "^1.0.0",
    "cross-fetch": "^4.0.0"
  },
  "jest": {
    "rootDir": ".",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx,js}"
    ],
    "verbose": true,
    "testMatch": [
      "<rootDir>/src/**/*.test.{ts,tsx,js}"
    ],
    "transform": {
      "\\.[jt]sx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "allowSyntheticDefaultImports": true,
            "esModuleInterop": true,
            "allowJs": true,
            "target": "ESNext",
            "jsx": "react-jsx"
          },
          "diagnostics": {
            "ignoreCodes": [
              1343
            ]
          },
          "astTransformers": {
            "before": [
              {
                "path": "ts-jest-mock-import-meta",
                "options": {
                  "metaObjectReplacement": {
                    "url": "JEST"
                  }
                }
              }
            ]
          }
        }
      ]
    },
    "moduleNameMapper": {
      "^(.+)\\.js$": "$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
